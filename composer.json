{
	"name": "maslosoft/cache",
	"description": "Easy to use, auto configurable, extensible cache provider",
	"license": [
		"AGPL",
		"Commercial"
	],
	"authors": [
		{
			"name": "Peter Maselkowski",
			"email": "pmaselkowski@gmail.com"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"maslosoft/embedi": "^1|^2|^3",
		"php": ">=7.4"
	},
	"require-dev": {
		"codeception/codeception": "^5",
		"codeception/module-asserts": "^3",
		"ext-json": "*"
	},
	"autoload": {
		"psr-4": {
			"Maslosoft\\Cache\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Maslosoft\\CacheTest\\Testers\\": "tests/testers"
		}
	},
	"suggest": {
		"ext-apcu": "Use APCu as cache",
		"ext-memcached": "Use Memcached as cache"
	},
	"homepage": "https://maslosoft.com/cache/"
}
